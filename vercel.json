{
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/", "destination": "/ko" },
    { "source": "/blog/:path*", "destination": "/ko/blog/:path*" },
    { "source": "/projects/:path*", "destination": "/ko/projects/:path*" },
    { "source": "/docs/:path*", "destination": "/ko/docs/:path*" },
    { "source": "/about", "destination": "/ko/about" },
    { "source": "/p/:path*", "destination": "/ko/p/:path*" }
  ]
}
